select t1.EMPLOYEE_ID,t2.EMPLOYEE_2019  from EMPLOYEE_2019 t2,employee t1 where DEPARTMENT_ID=20 union select t1.EMPLOYEE_ID,t2.EMPLOYEE_2019  from  EMPLOYEE_2019 t2,employee t1 where DEPARTMENT_ID=20;

select* FROM employee where DEPARTMENT_ID=20 UNION SELECT * FROM EMPLOYEE_2019 WHERE DEPARTMENT_ID=20;

select* FROM employee where DEPARTMENT_ID=20 UNION ALL SELECT * FROM EMPLOYEE_2019 WHERE DEPARTMENT_ID=20;

select * FROM employee WHERE EMPLOYEE_ID IN ( SELECT EMPLOYEE_ID  FROM EMPLOYEE_2019 );
SELECT * FROM employee Minus select * from EMPLOYEE_2019;
