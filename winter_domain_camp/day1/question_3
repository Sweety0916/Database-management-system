CREATE TABLE EMPLOYEE_2019(
EMPLOYEE_ID INT,
EMPLOYEE_NAME VARCHAR(20),
JOB VARCHAR(20),
MANAGER_ID INT,
HIREDATE DATE,
SALARY INT,
COMMISSION INT,
DEPARTMENT_ID INT);

